package mySwitch;
import java.util.Scanner;
public class ExampleSwitch1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int x;
		Scanner sc=new Scanner (System.in);
		System.out.print ("Enter the number of x: ");
		x=sc.nextInt();
		
		//break; only print out the output is correct
		//not put the break; will print out all the output
		switch (x) {
		case 10:
			System.out.println ("x is 10");
			break;
		case 20:
			System.out.println ("x is 20");
			break;
		case 30:
			System.out.println ("x is 30");
			break;
		default:
			System.out.println ("x is not 10 or 20 or 30");
		}
		
		sc.close();
		
	}

}
