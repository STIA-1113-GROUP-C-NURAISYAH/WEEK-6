package mySwitch;

import java.util.Scanner;

public class Marks {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner (System.in);
		System.out.print ("Enter mark: ");
		int mark=sc.nextInt();
		mark=mark/10; //divide 10 to get range mark in case
		
		switch (mark) { 
		
		case 10:
		case 9:
			System.out.println ("Excellent: Grade A");
			break;
		case 8:
			System.out.println ("Very Good: Grade B");
			break;
		case 7:
			System.out.println ("Good: Grade C");
			break;
		case 6:
			System.out.println ("Good: Grade D");
			break;
		case 5:
			System.out.println ("Good: Grade E");
			break;
		case 4:
			System.out.println ("Passed: Grade F");
			break;
		default :
			System.out.println ("Failed!");
			
		}
		sc.close();
	}

}
